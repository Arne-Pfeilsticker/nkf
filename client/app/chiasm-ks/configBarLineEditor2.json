{
  "layout": {
    "plugin": "chiasm/plugins/layout",
    "state": {
      "layout": {
        "orientation": "horizontal",
        "children": [
          "editor",
          {
            "orientation": "vertical",
            "children": [
              "barChart",
              "lineChart"
            ]
          }
        ]
      }
    }
  },
  "editor": {
    "plugin": "chiasm/plugins/configEditor",
    "state": {
      "size": "325px"
    }
  },
  "barChart": {
    "plugin": "chiasm/plugins/barChart",
    "state": {
      "xColumn": "letter",
      "xAxisLabel": "Letter",
      "yColumn": "frequency",
      "yAxisLabel": "Frequency",
      "margin": {
        "top": 32,
        "right": 2,
        "bottom": 40,
        "left": 47
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "colorDefault": "#009e73",
      "title": "Hello",
      "titleOffset": -0.269568,
      "yDomainMin": 0
    }
  },
  "lineChart": {
    "plugin": "chiasm/plugins/lineChart",
    "state": {
      "xColumn": "timestamp",
      "xAxisLabel": "Time",
      "yColumn": "temperature",
      "yAxisLabel": "Temperature",
      "margin": {
        "top": 30,
        "right": 2,
        "bottom": 40,
        "left": 47
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "colorDefault": "#0074db",
      "title": "Line Chart",
      "titleOffset": -0.30433148928
    }
  },
  "barChartCSVLoader": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "scripts/chiasm-ks/simpleBars.csv",
      "numericColumns": [
        "frequency"
      ]
    }
  },
  "lineChartCSVLoader": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "scripts/chiasm-ks/simpleTime.csv",
      "numericColumns": [
        "temperature"
      ],
      "timeColumns": [
        "timestamp"
      ]
    }
  },
  "links": {
    "plugin": "chiasm/plugins/links",
    "state": {
      "bindings": [
        "barChartCSVLoader.data -> barChart.data",
        "lineChartCSVLoader.data -> lineChart.data"
      ]
    }
  }
}
