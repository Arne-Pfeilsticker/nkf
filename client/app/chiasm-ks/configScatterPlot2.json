{
  "layout": {
    "plugin": "chiasm/plugins/layout",
    "state": {
      "layout": {
        "orientation": "horizontal",
        "children": [
          "editor",
          "scatterPlot"
        ]
      }
    }
  },
  "editor": {
    "plugin": "chiasm/plugins/configEditor",
    "state": {
      "size": "325px"
    }
  },
  "scatterPlot": {
    "plugin": "chiasm/plugins/scatterPlot",
    "state": {
      "xColumn": "sepal_length",
      "xAxisLabel": "Sepal Length",
      "yColumn": "sepal_width",
      "yAxisLabel": "Sepal Width",
      "margin": {
        "top": 32,
        "right": 2,
        "bottom": 40,
        "left": 47
      },
      "xAxisLabelOffset": 1.9,
      "yAxisLabelOffset": 1.4,
      "title": "Iris",
      "titleOffset": -0.269568,
      "sizeDefault": 12
    }
  },
  "barChartCSVLoader": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "scripts/chiasm-ks/simpleBars.csv",
      "numericColumns": [
        "frequency"
      ]
    }
  },
  "iris": {
    "plugin": "chiasm/plugins/csvLoader",
    "state": {
      "csvPath": "scripts/chiasm-ks/iris.csv",
      "numericColumns": [
        "sepal_length",
        "sepal_width",
        "petal_length",
        "petal_width"
      ]
    }
  },
  "links": {
    "plugin": "chiasm/plugins/links",
    "state": {
      "bindings": [
        "iris.data -> scatterPlot.data"
      ]
    }
  }
}
